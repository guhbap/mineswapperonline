{
  "name": "test-vite-vue",
  "version": "1.24.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "prebuild": "node -p \"'VITE_APP_VERSION=' + require('./package.json').version\" > .env",
    "build": "vite build",
    "build:dev": "npm run type-check && vite build --mode devbuild",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/components": "^12.7.0",
    "@vueuse/core": "^12.7.0",
    "air-datepicker": "^3.5.3",
    "axios": "^1.7.9",
    "chart.js": "^4.4.9",
    "chartjs-chart-funnel": "^4.2.4",
    "chartjs-plugin-datalabels": "^2.2.0",
    "choices.js": "^11.1.0",
    "fuse.js": "^7.1.0",
    "pinia": "^3.0.1",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.2",
    "vue-final-modal": "^4.5.5",
    "vue-router": "^4.5.0",
    "vue3-virtual-scroll-list": "^0.2.1"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-oxlint": "~1.8.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.8.0",
    "prettier": "3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
